<?xml version="1.0" encoding="UTF-8"?>
<Window size="220,120" roundcorner="2,2">
	<VerticalLayout bkcolor="#FFFFFFFF" bordercolor="#FFD9D9D9" bordersize="1,1,1,1">
		<HorizontalLayout height="24" padding="0,10,0,5">
			<Control />
			<Label text="提示" align="center"  height="24" font="1" textcolor="#FF000000"/>
			<Control />
		</HorizontalLayout>
		<HorizontalLayout height="24" padding="0,5,0,10">
			<Label name="msgText" padding="10,0,10,0" align="center" height="24" font="0" textcolor="#FF000000"/>
		</HorizontalLayout>
		<Control height="1" bkcolor="#FFDBDBDD" />
		<HorizontalLayout height="40">
			<Container>
				<Button name="msgCancel" text="取消" height="40" font="0" cursor="hand" textcolor="#FF6A6969"/>
			</Container>
			<Control height="40" width="1" bkcolor="#FFDBDBDD" />
			<Container>
				<Button name="msgConfirm" text="确定" height="40" font="0" cursor="hand" textcolor="#FF000000"/>
			</Container>
		</HorizontalLayout>
	</VerticalLayout>
</Window>