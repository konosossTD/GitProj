<?xml version="1.0" encoding="UTF-8"?>
<Window>
  <ListContainerElement height="66" >
	<VerticalLayout>
		<Control />
		<Label name="fileNameLabel" endellipsis="true" align="left" textcolor="#FF464546" font="0" />
		<Control />
	</VerticalLayout>
	<Container>
		<Label name="fileSizeLabel" align="center" textcolor="#FF464546" font="0" />
	</Container>
	<Container>
		<Label name="durationLabel" align="center" textcolor="#FF464546" font="0" />
	</Container>
	<HorizontalLayout>
		<Control />
		<VerticalLayout height="66" width="110">
			<Control />
			<Slider name="convert-progress" value="0" step="1" width="110" height="12" enabled="false" bkimage="file='progress-slider-bk.png'" foreimage="file='progress-slider-fore.png'" />
			<Control />
		</VerticalLayout>
		<Label name="progress-label" width="40" align="center" textcolor="#FF464546" font="0" />
		<Control />
	</HorizontalLayout>
	<VerticalLayout>
		<Control />
		<HorizontalLayout height="30">
			<Control />
			<HorizontalLayout width="100" height="30">
				<Button name="convertBtn" text="转 换" width="70" height="30" textcolor="#FFFFFFFF"  bkcolor="#FF27B765" hotbkcolor="#FF46C47C" cursor="hand" font="0" />
				<Button name="convertListMenuBtn" tooltip="更多操作" padding="4,0,0,0" width="32" height="32" bkimage="file='list-menu.png'"  cursor="hand" />
			</HorizontalLayout>
			<Control />
		</HorizontalLayout>
		<Control />
	</VerticalLayout>
  </ListContainerElement>
</Window>
